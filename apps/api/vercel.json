{
  "version": 2,
  "buildCommand": "pnpm install --frozen-lockfile && pnpm prisma generate && pnpm build",
  "outputDirectory": "dist",
  "framework": null,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
